<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nbb.mybatis.nbbmybatis.mapper.DictTypeMapper">
    <cache></cache>
    <insert id="save">
        insert into
            dict_type(id, dict_type, dict_name)
        values(#{id}, #{dictType}, #{dictName})
    </insert>


    <select id="list" resultType="com.nbb.mybatis.nbbmybatis.entiry.DictType">
        select * from dict_type
    </select>

    <select id="list2" resultType="com.nbb.mybatis.nbbmybatis.entiry.DictType">
        select * from dict_type
    </select>
</mapper>